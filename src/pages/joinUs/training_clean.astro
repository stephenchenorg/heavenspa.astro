            <ul class="page"></ul>
        </div>
    </div>

    <style>
        .pageIndex {
            max-width: 100%;
            padding: 0;
        }

        .main_part {
            max-width: 2000px;
            padding: 0;
            margin: 0 auto;
        }

        .edit,
        .edit_part,
        .show_content,
        .main_part {
            width: 100%;
            max-width: 100%;
            padding: 0;
        }

        .edit * {
            word-break: normal;
        }

        .banner {
            display: none;
        }

        body:before {
            content: ' ';
            position: fixed;
            z-index: -1;
            inset: 0;
            background-position: center;
            background-repeat: no-repeat;
            background-size: cover;
            background-image: url(https://pic03.eapple.com.tw/yonghe32/bg.jpg);
        }

        .animated15 {
            animation: none;
        }

        .box_container {
            width: 100%;
            max-width: 2000px;
            font-family: "微軟正黑體", Microsoft JhengHei;
            font-size: var(--font18);
            letter-spacing: 1.5px;
            color: #000000;
            border: 0;
            text-decoration: none;
            overflow: hidden;
            margin: auto;
            --font80: 80px;
            --font30: 30px;
            --font26: 26px;
            --font20: 20px;
            --font18: 18px;
            --font17: 17px;
        }

        .box_container img {
            width: auto;
            max-width: 100%;
        }

        .box_container * {
            box-sizing: border-box;
            max-width: 100%;
            padding: 0;
            margin: 0;
            line-height: 120%;
        }

        .box_container a {
            text-decoration: none;
        }

        .box_container ul {
            list-style: none;
        }

        .box_container i,
        .box_container em,
        .box_container dfn,
        .box_container var {
            font-style: normal;
            line-height: 0px;
        }

        .box_container u {
            text-decoration: none;
        }

        /* Animations */
        [data-animate-in] {
            opacity: 0;
            transition: transform 0.8s ease, opacity 0.8s ease;
        }

        [data-animate-in="up"] {
            transform: translate3d(0, 20%, 0);
        }

        [data-animate-in="left"] {
            transform: translate3d(-30%, 0, 0);
        }

        [data-animate-in="right"] {
            transform: translate3d(30%, 0, 0);
        }

        [data-animate-in="down"] {
            transform: translate3d(0, -20%, 0);
        }

        [data-animate-in="fadeIn"] {
            transform: translate3d(0, 0, 0);
        }

        [data-animate-in="scaleIn"] {
            transform: scale(.3);
        }

        [data-animate-in].in-view {
            opacity: 1;
            transform: translate3d(0, 0, 0);
            -webkit-transform: translate3d(0, 0, 0);
            transition: transform 1.2s ease, opacity 1s ease;
        }

        .fade-in {
            opacity: 0;
            transition: opacity 1s ease;
        }

        .content_flex {
            display: flex;
            justify-content: space-between;
            align-items: center;
        }

        /* 主要區塊樣式 */
        .cabg01 {
            background: url(https://pic03.eapple.com.tw/yonghe32/cbg_01.jpg) no-repeat center;
            background-size: cover;
            padding-top: 100px;
        }

        .cabg01_1 {
            padding-left: 3%;
            margin-left: auto;
            max-width: calc(1640px + 3%);
            justify-content: flex-end;
            align-items: stretch
        }

        .cabg01_1 dt {
            width: 600px;
            flex-wrap: wrap;
            display: flex;
            justify-content: flex-start;
            align-items: center;
            align-content: space-between;
        }

        .cabg01_1 dt section {
            margin: 5% 0;
        }

        .cabg01_1 dt section figure {
            display: flex;
            justify-content: flex-start;
            align-items: stretch;
            margin-top: 20px;
        }

        .cabg01_1 dt section figure a {
            display: inline-block;
            padding: 7px 15px;
            background: #9A9972;
            color: #fff;
            font-weight: bold;
            font-size: var(--font20);
            letter-spacing: 0;
            border-radius: 50px;
        }

        .cabg01_1 dt section figure a + a {
            margin-left: 13px;
        }

        .cabg01_1 dt section figure a:last-child,
        .cabg01_1 dt section figure a:hover {
            background: #9A9972;
            color: #fff;
        }

        .cabg01_1 dt p {
            padding: 55px 0;
            position: relative;
            z-index: 1;
        }

        .cabg01_1 dt p:before {
            content: '';
            background: rgba(255, 255, 255, .5);
            position: absolute;
            left: 30%;
            top: 0;
            width: 1230px;
            height: 100%;
            transform: translateX(-50%);
            z-index: -1;
            border-top-right-radius: 200px;
        }

        .cabg01_1 dt p span {
            display: block;
            margin-top: 20px;
            font-size: var(--font17);
            max-width: 390px;
        }

        .cabg01_1 dd {
            line-height: 0;
        }

        .cabg01_1 dd img {
            object-fit: cover;
            object-position: left bottom;
            min-height: 100%;
        }

        .cabg01_1 dd img:last-child {
            display: none;
        }

        /* 響應式設計 */
        @media screen and (max-width: 1024px) {
            .cabg01_1 dt section figure a {
                font-size: 17px;
                padding: 8px;
            }
        }

        @media screen and (max-width: 768px) {
            .cabg01 {
                padding-top: 70px;
            }

            .cabg01_1 {
                flex-direction: column;
                align-items: center;
                width: 90%;
                padding-left: 0;
                margin: auto;
            }

            .cabg01_1 dt section {
                margin: 0 auto 30px;
            }

            .cabg01_1 dt section figure {
                justify-content: center;
            }

            .cabg01_1 dt p {
                padding: 50px 5%;
            }

            .cabg01_1 dt p:before {
                width: 100%;
                transform: unset;
                left: 0;
                border-radius: 0;
            }

            .cabg01_1 dd img:first-child {
                display: none;
            }

            .cabg01_1 dd img:last-child {
                display: block;
                margin: 30px auto;
            }
        }

        @media screen and (max-width: 480px) {
            .box_container {
                --font80: 40px;
                --font30: 25px;
                --font26: 20px;
                --font20: 18px;
                --font18: 16px;
                --font17: 16px;
            }

            .cabg01 {
                padding-top: 50px;
            }

            .cabg01_1 dt section > img {
                max-width: 300px;
            }

            .cabg01_1 dt section figure a + a {
                margin-left: 8px;
            }

            .cabg01_1 dt p {
                padding: 40px 5%;
            }
        }
    </style>
</Layout>
