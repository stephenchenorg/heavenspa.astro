<style>
  .theme-icon {
    margin: 0;
    padding: 0;
    color: var(--theme-text, #000000);
    transition: color 0.3s ease;
  }

  .dark .theme-icon,
  .dark-theme .theme-icon,
  [data-theme="dark"] .theme-icon {
    color: var(--theme-text, #ffffff);
  }

  .theme-icon:hover {
    opacity: 0.7;
    transform: scale(1.05);
    transition: all 0.2s ease;
  }
</style>